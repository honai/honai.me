{
  "version": 2,
  "name": "portfolio",
  "builds": [
    { "src": "next.config.js", "use": "@now/next" },
    { "src": "api/atom.ts", "use": "@now/node" },
    { "src": "api/rss.ts", "use": "@now/node" }
  ],
  "build": {
    "env": {
      "CTF_SPACE_ID": "7q1ibtbymdj9",
      "CTF_ACCESSTOKEN": "@contentful-delivery"
    }
  },
  "env": {
    "CTF_SPACE_ID": "7q1ibtbymdj9",
    "CTF_ACCESSTOKEN": "@contentful-delivery"
  },
  "regions": ["hnd1"],
  "alias": ["honai.me"],
  "routes": [
    {
      "src": "/blog/post/(?<slug>[\\S]+)$",
      "dest": "/blog/post?slug=$slug"
    },
    {
      "src": "/feed",
      "dest": "/api/atom"
    },
    {
      "src": "/rss",
      "dest": "/api/rss"
    },
    {
      "src": "/post/(?<slug>[\\S]+)",
      "status": 301,
      "headers": { "Location": "https://honai.me/blog/post/$slug" }
    }
  ]
}
