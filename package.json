    "wrangler": "^2.0.22"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-fetch": "^3.0.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.1.0",
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
